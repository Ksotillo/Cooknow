{
  "expo": {
    "name": "Cooknow",
    "description": "React Native App for Software Engineering",
    "slug": "cooknow",
    "privacy": "unlisted",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#6C24AA"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cspyk.cooknow"
    },
    "android": {
      "package": "com.cspyk.cooknow",
      "googleServicesFile": "./google-services.json"
    }
  }
}
